{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.PostgreSQL.Alternative" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "PostgreSQL",
        "Args": {
          "connectionString": "Host=localhost;Port=5432;Database=logsdb;Username=admin;Password=StrongPassword123;",
          "tableName": "Logs",
          "needAutoCreateTable": true,
          "batchPostingLimit": 50,
          "period": "00:00:10"
        }
      },
      {
        "Name": "PostgreSQL",
        "Args": {
          "connectionString": "Host=localhost;Port=5432;Database=logsdb;Username=admin;Password=StrongPassword123;",
          "tableName": "ErrorLogs",
          "needAutoCreateTable": true,
          "batchPostingLimit": 50,
          "period": "00:00:10",
          "restrictedToMinimumLevel": "Error"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "EC"
    }
  }
}
